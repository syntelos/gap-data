<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">

  <display-name>@ProjectTarget@</display-name>

  <servlet>
    <servlet-name>A</servlet-name>
    <servlet-class>tt.AServlet</servlet-class>
    <load-on-startup>-1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Person</servlet-name>
    <servlet-class>tt.PersonServlet</servlet-class>
    <load-on-startup>-1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>Channel</servlet-name>
    <servlet-class>tt.ChannelServlet</servlet-class>
    <load-on-startup>-1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>B</servlet-name>
    <servlet-class>tt.BServlet</servlet-class>
    <load-on-startup>-1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>C</servlet-name>
